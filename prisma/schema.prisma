// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usd_jpy_1min {
  datetime DateTime?
  unixtime BigInt @id
  open Float
  high Float
  low Float
  close Float
  volume BigInt
  up Boolean @default(false)
  down Boolean @default(false)
}
